first_10_lower <- letters[1:10]
last_10_upper <- toupper(letters[17:26])
letters_between_22_to_24_upper <- toupper(letters[22:24])
cat("First 10 lowercase letters:", first_10_lower, "\n")
cat("Last 10 uppercase letters:", last_10_upper, "\n")
cat("Letters between 22nd to 24th in uppercase:", letters_between_22_to_24_upper, "\n")
