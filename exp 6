sieve_of_eratosthenes <- function(n) {
  is_prime <- rep(TRUE, n + 1)
  p <- 2
  while (p^2 <= n) {
    if (is_prime[p]) {
      for (i in (p^2):n) {
        if (i %% p == 0) {
          is_prime[i] <- FALSE
        }
      }
    }
    p <- p + 1
  }
  primes <- which(is_prime)
  primes <- primes[primes > 1]  
  return(primes)
}
n <- 10
prime_numbers <- sieve_of_eratosthenes(n)
cat("Prime numbers up to", n, "are:", prime_numbers, "\n")
